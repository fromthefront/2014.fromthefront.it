<section class="ticketab__section">
    <div class="row">
        <section class="ufo-aside">
            <%= partial "history" %>
        </section>
        <article class="ticketab">
           <div class="ticketab__inner">
                <header class="ticketab__header">
                    <h1 class="ticketab__title"><%= t(:geticket) %></h1>
               </header>
                <p class="ticketab__text"><%= t(:ticketext) %></p>
           </div>
            <table class="ticketable">
                <thead class="ticketable__head">
                    <tr>
                      <th class="ticketable__heads"></th>
                      <th class="ticketable__heads head--singleday"><%= t(:single) %> *</th>
                      <th class="ticketable__heads head--twodays"><%= t(:pass) %></th>
                      <th class="ticketable__heads"><%= t(:available) %></th>
                    </tr>
                </thead>
                <tbody class="ticketable__body">
                    <tr class="ticketable__row">
                        <td class="ticketable__first"><s>blind bird</s></td>
                        <td>&mdash;</td>
                        <td><s>€70</s></td>
                        <td><%= t(:soldout) %></td>
                    </tr>
                    <tr class="ticketable__row">
                        <td class="ticketable__first"><s>early bird</s></td>
                        <td>&mdash;</td>
                        <td><s>€100</s></td>
                        <td><%= t(:soldout) %></td>
                    </tr>
                    <tr class="ticketable__row">
                        <td class="ticketable__first">regular ticket</td>
                        <td><s>€105<s></td>
                        <td><s>€130</s></td>
                        <td><%= t(:soldout) %></td>
                    </tr>
                    <tr class="ticketable__row">
                        <td class="ticketable__first">late bird</td>
                        <td>€135</td>
                        <td>€160</td>
                        <td><%= t(:augsep) %></td>
                    </tr>
                </tbody>
                <tfoot class="ticketable__footer">
                    <tr>
                        <td class="ticketable__notes" colspan="4">
                         <%= t(:discl1) %><br /><br class="show-for-medium-up" /><%= t(:discl2) %>
                        </td>
                    </tr>
                </tfoot>
            </table>
        </article>
    </div>
</section>
